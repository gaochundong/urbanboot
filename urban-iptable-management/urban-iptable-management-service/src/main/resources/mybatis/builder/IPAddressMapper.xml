<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ai.sangmado.urbanboot.urban.iptable.management.mapper.IPAddressMapper">

    <select id="queryIPAddress" resultType="ai.sangmado.urbanboot.urban.iptable.management.entity.IPAddressPO">
        select * from ip_address where id = #{iptableId} limit 1
    </select>

    <select id="queryIPAddressList" resultType="ai.sangmado.urbanboot.urban.iptable.management.entity.IPAddressPO">
        select * from ip_address where parent_id = #{parentId} order by pinyin
    </select>

</mapper>